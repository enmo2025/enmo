model Event {
  id                String @id @unique @default(cuid())
  title             String
  description       String
  eventBanner       String
  content           String
  participantFee    String
  date              DateTime
  location          String
  companyName       String
  companyProfile    String
  companyLogo       String
  hostName          String
  purchases         Purchase[]
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
} 